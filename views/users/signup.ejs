<% layout ("/layouts/boilerplate")-%>

<div class="row mt-3 ">
    <div class=" col-6 offset-3 mb-1" >
        
    <h2 class="col-6 offset-3">SignUp For TripNest</h2>
 <form method="POST" action="/signup" novalidate class="needs-validation">
   <div  class="mb-3">
    <lable for="username" class="form-lable">Username</lable>
        <input  name="username"  
        type="text" 
      id="username"
        class="form-control"
        required/>
        <div class="valid-feedback">Looks good!</div>

   </div> 

   <div  class="mb-3">
    <lable for="email" class="form-lable">Email</lable>
        <input  name="email"  
        type="text" 
      id="email"
        class="form-control"
        required/>
       

   </div> 
   <div  class="mb-3">
    <lable for="username" class="form-lable">Password</lable>
        <input  name="password"  
        type="password" 
       id="password"
        class="form-control"
        required/>
        

   </div> 
   <button class="btn btn-success mb-3">SignUp</button>
        </form>
 
    </div>
</div> 