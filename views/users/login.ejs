<% layout ("/layouts/boilerplate")-%>

<div class="row mt-3">
    <div class=" col-6 offset-3" >
        
    <h2 class="col-6 offset-3">Login For TripNest</h2>
 <form method="POST" action="/login" novalidate class="needs-validation">
   <div  class="mb-3">
    <lable for="username" class="form-lable">Username</lable>
        <input  name="username"  
        type="text" 
      id="username"
        class="form-control"
        required/>
        

   </div> 

   
   <div  class="mb-3">
    <lable for="username" class="form-lable">Password</lable>
        <input  name="password"  
        type="password" 
       id="password"
        class="form-control"
        required/>
        

   </div> 
   <button class="btn btn-success mb-3">Login</button>
        </form>
 
    </div>
</div> 